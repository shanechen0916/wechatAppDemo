<view class="userinfo" bindtap="login">
  <image class="userinfo-avatar" src="{{logged ? userInfo.avatarUrl : './user-unlogin.png'}}" background-size="cover">
  </image>
  <span class="userinfo-nickname">
    {{logged ? userInfo.nickName : '点击登录'}}
  </span>
</view>

